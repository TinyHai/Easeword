<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.tyhyh.easeword">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoToolBar"
        android:name="cn.tyhyh.easeword.EasyWordApplication">
        <activity android:name="cn.tyhyh.easeword.ui.activity.SplashActivity"
            android:theme="@style/AppThemeNoToolBar.SplashActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.tyhyh.easeword.ui.activity.MainActivity" />
    </application>
</manifest>
