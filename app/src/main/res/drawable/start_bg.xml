<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
        </shape>
    </item>
    <item
        android:top="-100dp"
        android:right="-20dp"
        android:gravity="right|top">
        <shape android:shape="oval">
            <size
                android:width="200dp"
                android:height="200dp" />
            <solid android:color="#193391ff" />
        </shape>
    </item>
    <item
        android:gravity="center_vertical|right"
        android:right="51dp">
        <shape android:shape="oval">
            <size
                android:width="70dp"
                android:height="70dp" />
            <solid android:color="#0af24645" />
        </shape>
    </item>
    <item
        android:gravity="left|bottom"
        android:left="-40dp"
        android:bottom="100dp">
        <shape android:shape="oval">
            <size
                android:height="140dp"
                android:width="140dp" />
            <solid android:color="#19fbb929" />
        </shape>
    </item>
    <item
        android:gravity="center"
        android:drawable="@mipmap/start_logo" />
</layer-list>